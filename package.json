{
  "name": "pslf-calculations",
  "version": "1.0.1",
  "description": "PSLF calculations for the student loan forgiveness program",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./coverage",
    "start": "node ./lib/index.ts",
    "compile": "tsc",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch",
    "jest": "jest --coverage",
    "format:fix": "prettier --config .prettierrc 'lib/**/*.ts' --write",
    "eslint": "eslint --ext .ts lib/",
    "eslint:fix": "eslint --ext .ts lib/ --fix",
    "test": "npm run format:fix && npm run eslint:fix && npm run jest"
  },
  "author": "Marlena Alhayani <marlena.alhayani@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/malhayani/pslfCalculations.git"
  },
  "license": "ISC",
  "dependencies": {
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0"
  }
}